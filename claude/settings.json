{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_SUBAGENT_MODEL": "opus[1m]",
    "DISABLE_BUG_COMMAND": "1",
    "DISABLE_COST_WARNINGS": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_TELEMETRY": "1"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "Edit(*)",
      "Write(*)",
      "Update(*)",
      "Read",
      "Bash(mv:*)",
      "Bash(mkdir:*)",
      "Bash(pnpm:*)",
      "Bash(pnpx:*)",
      "Bash(git:*)",
      "Bash(gt:*)",
      "Bash(gs:*)",
      "Bash(gh:*)",
      "Bash(bazel:*)",
      "Bash(git:*)",
      "Bash(bazel:*)",
      "Bash(mkdir:*)",
      "Bash(go:*)",
      "Bash(ls:*)",
      "Bash(echo:*)",
      "Bash(sed:*)",
      "Bash(source:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(pkill:*)",
      "Bash(touch:*)",
      "Bash(grep:*)",
      "Bash(curl:*)",
      "Bash(rg:*)",
      "Bash(chmod:*)",
      "Bash(lsof:*)",
      "Bash(find:*)",
      "Bash(docker:*)",
      "Bash(python:*)",
      "Bash(python3:*)",
      "mcp__chrome-devtools__*"
    ],
    "deny": [
      "Bash(rm -rf /)",
      "Read(.env)",
      "Read(.env.*)"
    ],
    "ask": [
      "Bash(aws:*)",
      "Bash(kubectl:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(rm:*)"
    ],
    "defaultMode": "acceptEdits"
  },
  "model": "opus[1m]",
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true,
    "gopls-lsp@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "github@claude-plugins-official": true,
    "ralph-wiggum@claude-plugins-official": true,
    "rust-analyzer-lsp@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754077506421
  },
  "effortLevel": "medium"
}
